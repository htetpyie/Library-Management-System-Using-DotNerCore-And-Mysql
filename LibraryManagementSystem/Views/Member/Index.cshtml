@{
    ViewData["Title"] = "Create Member";
}
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h2 class="jaoas-label">Member</h2>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item sub-label">Member</li>
                </ol>
            </div>

        </div>
    </div>
</section>

<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col">
                <div class="card">
                    <div class="card-header">
                        <a asp-action="CreateMember">Create Member</a>
                    </div>
                    <div class="card-body">
                        <table id="member-table">
                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>First Name</th>
                                    <th>Last Name</th>
                                    <th>Email</th>
                                    <th>Phone</th>
                                    <th>Address</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</section>

@section Scripts {
    <script>
    $(document).ready(function () {
        showModal("#model", "Member",'@TempData["Message"]');
    })
    </script>
    <script src="~/js/Member/member.js"></script>
}