<script>
    let showModal = (id, modalTitle, message)=>{
        if (message != "") {
            $('#modal-title').text(modalTitle);
            $('#modal-message').text(message);
            let myModal = new bootstrap.Modal(document.querySelector(id), {
                keyboard: false
            })
            myModal.show();
        }
    }

    let showDeleteModal = (deleteId) => {
        $('#modal-title').text("Book Delete");
        $('#delete-form #id').val(deleteId);
        $("#deleteModal").show();
    }

    $(document).ready(() => {
        $('.date-picker').datepicker({
            format: "dd-MM-yyyy",
            changeYear: true,
            changeMonth: true,
            changeDay: true,
            startView: "days",
            minViewMode: "days",
            autoclose: true
        });
    } )
   
</script>